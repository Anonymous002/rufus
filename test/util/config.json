{
  "formatters": {
    "basic": "basic: %(message)s",
    "custom": {
      "formatFn": "function format(record) { return record.message.toUpperCase(); }"
    }
  },
  "filters": {
    "s": "foo",
    "r": {
      "regex": "foo",
      "flags": "gi"
    },
    "f": {
      "function": "function filter(record) { return record.args.length > 2; }"
    }
  },
  "handlers": {
    "empty": {
      "class": "intel/handlers/null",
      "formatter": "custom",
      "filters": ["s", "r", "f"]
    },
    "spy": {
      "class": "./test/util/spyhandler"
    }
  },
  "loggers": {
    "test.config.json": {
      "level": "INFO",
      "propagate": false,
      "handlers": ["empty", "spy"]
    }
  }
}
